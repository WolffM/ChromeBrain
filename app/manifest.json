{
  "manifest_version": 2,

  "name": "ChromeBrain",
  "description": "Chrome Brain will store answers you have found in google searches for you to retrieve at a later time.",
  "version": "1.0",


  "icons": {
      "16": "chromeBrain.png",
      "128": "chromeBrainMain.png"
  },

  "browser_action": {
    "default_title": "Make this page red",
    "default_icon":  "chromeBrain.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "history",
    "activeTab",
    "storage"
  ]
}
